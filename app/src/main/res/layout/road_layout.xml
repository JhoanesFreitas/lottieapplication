    <ImageView
        android:id="@+id/bottom_road_1"
        style="@style/LottieApp.Road.Vertical.Small"
        android:layout_marginEnd="@dimen/default_margin_end"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <RelativeLayout
        android:id="@+id/bottom_step_1"
        style="@style/LottieApp.Road.Step.Context"
        app:layout_constraintBottom_toTopOf="@+id/bottom_road_1"
        app:layout_constraintEnd_toEndOf="@+id/bottom_road_1">

        <include layout="@layout/layout_vertical_step" />

        <include layout="@layout/text_view_number" />

        <include layout="@layout/layout_elephant" />

    </RelativeLayout>

    <ImageView
        android:id="@+id/bottom_curve_1"
        style="@style/LottieApp.Road.Curve.Top.Left"
        app:layout_constraintBottom_toTopOf="@+id/bottom_step_1"
        app:layout_constraintEnd_toEndOf="@+id/bottom_step_1" />

    <ImageView
        android:id="@+id/bottom_road_2"
        style="@style/LottieApp.Road.Horizontal"
        app:layout_constraintBottom_toBottomOf="@+id/bottom_curve_1"
        app:layout_constraintEnd_toStartOf="@+id/bottom_curve_1" />

    <ImageView
        android:id="@+id/bottom_curve_2"
        style="@style/LottieApp.Road.Curve.Bottom.Left"
